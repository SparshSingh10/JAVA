//1. WAP to input an array of N number of elements and display it.



#include<stdio.h>
int main()
{
	int i,n;
	printf("enter size\n");
	scanf("%d",&n);
	int a[n];
	printf("enter element\n");
	for(i=0;i<n;i++)
	{
		
		scanf("%d",&a[i]);
	}
	printf("elements are\n");
		for(i=0;i<n;i++)
	{
		printf("%d ",a[i]);
	}			
}
