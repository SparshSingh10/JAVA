import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int n1=sc.nextInt();
        int n2=sc.nextInt();
        int c=0,i=1;
        while(true){
            int s=3*i+2;
            if((s%n2)!=0){
                System.out.println(s);
                s=0;
                c++;
            }
            i++;
            if(c==10)
                break;
        }

    }
}
