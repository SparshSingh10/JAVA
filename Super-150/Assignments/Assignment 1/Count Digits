import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int x=sc.nextInt();
        int c=0;
        while(n!=0){
            int r=n%10;
            if(x==r)
                ++c;
            n=n/10;
        }
        System.out.println(c);
    }
}
