import java.util.*;
public class Main {
    public static void main(String args[]) {
Scanner sc=new Scanner(System.in);
int base=sc.nextInt();
int dest=sc.nextInt();
int n=sc.nextInt();
int m=0;
int d=0;
while(n>0){
    int r=n%10;
    int f=r*(int)(Math.pow(base,m));
    d=d+f;
    m=m+1;
    n=n/10;
}
int mul=1;
int sm=0;
while(d>0){
    int a=d%dest;
    sm=sm+(a*mul);
    mul=mul*10;
    d=d/dest;
}
System.out.println(sm);
    }
}
