import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int s=0;
        int n=sc.nextInt();
        s=n;
        while(s>=0){
            System.out.println(n);
             n=sc.nextInt();
            s+=n;
        }
    }
}
