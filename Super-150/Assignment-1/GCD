import java.util.*; 
class Main{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int divd=sc.nextInt();
        int div=sc.nextInt();
        int rem;
        while(divd%div!=0){
            rem=divd%div;
            divd=div;
            div=rem;
        }
        System.out.println(div);
    }
}
