import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int n1=n;
        int s=0;
        int d=(int)Math.floor(Math.log10(n) + 1);
        while(n>0){
            int r=n%10;
            s=s+(int)Math.pow(r,d);
            n=n/10;
        }
        if(s==n1)
            System.out.println("true");
        else
            System.out.println("false");
    }
}
