import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int c = 1;
    int s1 = 0;
    int s2 = 0;

    while(n > 0){
        int r = n%10;
        n = n/10;
        if(c%2 == 1){
            s1+= r;
        }
        else{
            s2+= r;
        }
        c++;
    }
System.out.println(s1);
System.out.println(s2);
}
}

