
//find-largest-and-smallest-of-three-numbers-in-java
import java.util.*;

public class maxormin {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        int max = largest(a, b, c);
        System.out.println("Largest = "+max);
        int min = min(a, b, c);
        System.out.println("Smalles = "+min);
    }
    public static int largest(int a,int b,int c) {  
        if (a>b && a>c)
            return a;
        else if (b>a && b>c)
            return b;
        else
            return c;
}
    public static int min(int a,int b,int c) {  
        if (a<b && a<c)
            return a;
        else if (b<a && b<c)
            return b;
        else
            return c;
}
}
