//to-check-area of circle
import java.util.*;
public class maxormin{
    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        int a=sc.nextDouble();
        System.out.println(area(a));
    }
        public static Double area(double a){
    
            return Math.PI*Math.pow(a,2);
    }
}
